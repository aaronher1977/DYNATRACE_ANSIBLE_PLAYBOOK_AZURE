bash -lc 'set -e; B=dynatrace-oneagent; mkdir -p "$B"/{files,group_vars,roles/dynatrace_oneagent/{tasks,defaults},inventories/{test/{group_vars},prod/{group_vars}}}; touch "$B"/{site.yml,README.md,.gitignore} "$B"/roles/dynatrace_oneagent/{README.md,defaults/main.yml,tasks/{main.yml,install_from_ivanti.yml,cleanup.yml}} "$B"/inventories/test/{hosts.ini,group_vars/all.yml} "$B"/inventories/prod/{hosts.ini,group_vars/all.yml} "$B"/group_vars/all.yml; echo -e \"# Dynatrace OneAgent Installer (Ivanti Source)\\nThis repo contains an Ansible playbook that downloads and installs Dynatrace OneAgent from Ivanti.\\n\" > \"$B\"/README.md'

bash -lc 'set -e; BASE="/opt/ansible/dynatrace-oneagent"; install -d "$BASE" "$BASE/files" "$BASE/group_vars" "$BASE/inventories/test/group_vars" "$BASE/inventories/prod/group_vars" "$BASE/roles/dynatrace_oneagent/tasks" "$BASE/roles/dynatrace_oneagent/defaults"; : > "$BASE/site.yml"; : > "$BASE/.gitignore"; : > "$BASE/README.md"; : > "$BASE/group_vars/all.yml"; : > "$BASE/inventories/test/hosts.ini"; : > "$BASE/inventories/test/group_vars/all.yml"; : > "$BASE/inventories/prod/hosts.ini"; : > "$BASE/inventories/prod/group_vars/all.yml"; : > "$BASE/roles/dynatrace_oneagent/README.md"; : > "$BASE/roles/dynatrace_oneagent/defaults/main.yml"; : > "$BASE/roles/dynatrace_oneagent/tasks/main.yml"; : > "$BASE/roles/dynatrace_oneagent/tasks/install_from_ivanti.yml"; : > "$BASE/roles/dynatrace_oneagent/tasks/cleanup.yml"; printf "# Dynatrace OneAgent Installer (Ivanti Source)\nThis repo contains an Ansible playbook that downloads and installs Dynatrace OneAgent from Ivanti.\n" > "$BASE/README.md"; echo "Created at: $BASE"'

